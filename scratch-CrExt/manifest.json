{
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "128.png",
    "default_title": "DevLab Listener"
  },
  "name": "DevLab",
  "description": "DevLab",
  "version": "0.1",
  "icons": {
    "128": "128.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "power"
  ],
  "background": {
    "scripts": [
      "firebase-2.2.1.min.js",
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "setup.js",
    "setup.html"
  ],
  "options_page": "setup.html"
}
